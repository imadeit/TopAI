<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="vld_support"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="$(VLDDIR)\include"
	/>
	<Tool
		Name="VCLinkerTool"
		AdditionalLibraryDirectories="$(VLDDIR)\lib\$(PlatformName)"
	/>
	<UserMacro
		Name="VldPlatformWin32"
		Value="Win32"
	/>
	<UserMacro
		Name="VldPlatformx64"
		Value="Win64"
	/>
</VisualStudioPropertySheet>
